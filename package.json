{
  "name": "cciyc-quasar",
  "version": "1.0.0",
  "description": "CCIYC Duty Management System",
  "productName": "CCIYC Duty Management System",
  "author": "Tim Choi <tim.c@aka-technology.com>",
  "private": true,
  "scripts": {
    "emu": "firebase emulators:start --import ./functions/latest --export-on-exit ./functions/latest",
    "build": "ENV_FILE=production quasar build",
    "build:dev": "ENV_FILE=development quasar dev",
    "build:prod": "ENV_FILE=production quasar build",
    "build:staging": "ENV_FILE=staging quasar build",
    "serve": "run-p emu build:dev",
    "serve:staging": "ENV_FILE=staging quasar dev",
    "serve:prod": "ENV_FILE=production quasar dev",
    "test:unit:ui": "vitest --ui",
    "test:unit": "vitest",
    "test:unit:ci": "vitest run",
    "deploy:prod": "firebase deploy --only hosting",
    "deploy:staging": "firebase hosting:channel:deploy event_test",
    "deploy": "run-s build:prod deploy:prod"
  },
  "dependencies": {
    "@apollo/client": "^3.10.4",
